---
import Hero from "@/components/home/Hero.astro";
import About from "@/components/home/about/About.astro";
import Advantage from "@/components/home/advantage/Advantage.astro";
import Apartaments from "@/components/home/apartaments/Apartaments.astro";
import Concept from "@/components/home/concept/Concept.astro";
import ContactUs from "@/components/home/contact/ContactUs.astro";
import Design from "@/components/home/design/Design.astro";
import Gallery from "@/components/home/gallery/Gallery.astro";
import Location from "@/components/home/location/Location.astro";
import Specifications from "@/components/home/specifications/Specifications.astro";
import Layout from "@/layouts/Layout.astro";
---

<Layout>
  <Hero />
  <Concept />
  <About />
  <Gallery />
  <ContactUs
    id="contact_individual"
    title="Запишись на індивідуальну консультацію"
    buttonText="записатися"
  />
  <Advantage />
  <Apartaments />
  <Design />
  <ContactUs
    id="get_free_plans"
    title="ОТРИМАЙ ПІДБІРКУ ВІЛЬНИХ ПЛАНУВАНЬ"
    buttonText="Отримати"
  />
  <Specifications />
  <Location />
  <ContactUs
    id="get_free_consultation"
    title="Зателефонуємо, проконсультуємо, підберемо найкращі варіанти"
    buttonText="зателефонуйте мені"
  />
</Layout>
